<span class="add">+        double lo = 25;</span>
<span class="add">+        double la = 55;</span>
<span class="add">+        int y = 0 , m = 0 , d = 0 , tz = 0;</span>
<span class="add">+</span>
<span class="add">+        DateTime cc = DateTime.Now;</span>
<span class="add">+        y = cc.Year;</span>
<span class="add">+        m = cc.Month;</span>
<span class="add">+        d = cc.Day;</span>
<span class="add">+        tz = TimeZone.CurrentTimeZone.GetUtcOffset(new DateTime (y,m,d)).Hours;</span>
<span class="add">+        String [] s ;</span>
<span class="add">+</span>
<span class="add">+        p.setCalcMethod ( 2 );</span>
<span class="add">+        p . setAsrMethod ( 0 );</span>
<span class="add">+        s = p . getDatePrayerTimes ( y , m , d , lo , la , tz );</span>
